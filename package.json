{
  "name": "falcon-sign",
  "version": "1.0.4",
  "description": "The JS module of the post-quantum digital signature algorithm (FALCON).",
  "main": "index.js",
  "scripts": {
    "build-falcon512-n3-v1": "emcc ./kernel/n3_v1/wasmapi.cc ./kernel/n3_v1/falcon/*.c -o ./kernel/n3_v1/wasmFile/falcon512.js",
    "build-falcon1024-n3-v1": "emcc ./kernel/n3_v1/wasmapi.cc ./kernel/n3_v1/falcon/*.c -o ./kernel/n3_v1/wasmFile/falcon1024.js -D FALCON=1024"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pqcsf/falcon-sign-js"
  },
  "keywords": [
    "PQC",
    "Cryptography",
    "Public-key cryptography"
  ],
  "author": "PQCSF",
  "license": "MIT",
  "homepage": "https://github.com/pqcsf/falcon-sign-js"
}
