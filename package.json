{
  "name": "falcon-sign",
  "version": "1.0.0",
  "description": "The JS module of the post-quantum digital signature algorithm (FALCON).",
  "main": "index.js",
  "scripts": {
    "bulid-falcon512-n3-v1": "emcc ./kernel/n3_v1/main.cc ./kernel/n3_v1/*.c ./kernel/n3_v1/falcon512/*.c -o ./kernel/n3_v1/falcon512/index.js",
	"bulid-falcon1024-n3-v1": "emcc ./kernel/n3_v1/main.cc ./kernel/n3_v1/*.c ./kernel/n3_v1/falcon1024/*.c -o ./kernel/n3_v1/falcon1024/index.js -D FALCON=1024"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pqcsf/falcon-sign-js"
  },
  "keywords": [
    "PQC",
    "Cryptography",
    "Public-key cryptography"
  ],
  "author": "PQCSF",
  "license": "MIT",
  "homepage": "https://github.com/pqcsf/falcon-sign-js"
}
